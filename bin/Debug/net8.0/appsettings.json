{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "DefaultConnection": "Data Source=prog7311poe.db"
  },
  "Debugger": {
    "EnableDebugMode": true,
    "LogLevel": "Verbose",
    "StackTraceEnabled": true,
    "OutputPath": "${workspaceFolder}/logs"
  },
  "DebuggerWatcher": [
    {
      "name": "Prog7311App", 
      "folder": ".", 
      "preLaunchCommand": "dotnet build", 
      "fileExtensions": [".cs", ".cshtml", ".razor"]
    }
  ]
}